{
  "name": "@jaalorsa/j5-components",
  "version": "1.5.3",
  "private": false,
  "description": "Libreria de componentes web para ayudar al desarrollador en su día a día. Como la librería está hecha en vanilla, se puede utilizar sin importar el framework de javascript.",
  "files": [
    "components"
  ],
  "type": "module",
  "types": "./components/main.d.ts",
  "main": "./components/j5-components.mjs",
  "exports": {
    ".": {
      "types": "./components/main.d.ts",
      "import": "./components/j5-components.mjs",
      "require": "./components/j5-components.umd.js"
    }
  },
  "author": "jaalorsa517",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaalorsa517/j5-components/issues"
  },
  "homepage": "https://jaalorsa.com/lib-components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaalorsa517/j5-components.git"
  },
  "keywords": [
    "webcomponents",
    "web",
    "component",
    "typescript",
    "javascript"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "test:watch": "vitest --watch"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.2",
    "@vitest/coverage-v8": "^2.0.5",
    "jsdom": "^25.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^4.0.3",
    "vitest": "^2.0.5"
  }
}
