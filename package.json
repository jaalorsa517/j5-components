{
  "name": "@jaalorsa/j5-components",
  "version": "1.5.0",
  "private": false,
  "description": "Libreria de componentes web para ayudar al desarrollador en su día a día. Como la librería está hecha en vanilla, se puede utilizar sin importar el framework de javascript.",
  "files": [
    "dist"
  ],
  "types": "./dist/main.d.ts",
  "main": "./dist/j5-components.umd.js",
  "exports": {
    ".": {
      "import": "./dist/j5-components.mjs",
      "require": "./dist/j5-components.umd.js"
    }
  },
  "author": "jaalorsa517",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaalorsa517/j5-components/issues"
  },
  "homepage": "https://github.com/jaalorsa517/j5-components#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaalorsa517/j5-components.git"
  },
  "keywords": [
    "webcomponents",
    "web",
    "component",
    "typescript",
    "javascript"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0"
  }
}
